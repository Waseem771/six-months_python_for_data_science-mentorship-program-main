; Add the buttons, initial distance from the left
X_Start = 0

SetFormat, Float, 0.0

Loop, Parse, Toolbar_Button_Function, `,
	{
	Button_Index := A_Index
	
	If A_LoopField = Color_Picker
		Color_Palette_Init = 1
		
	If A_LoopField = Timer_1
		Timer_1_Init = 1
	If A_LoopField = Timer_2
		Timer_2_Init = 1
	
	If A_LoopField = AddSlide_Picture
		{
		If !FileExist(General_Background_Picture_Path)
			{
			Msgbox, 0x40044,, Button #%A_Index% of your main toolbar is a "AddSlide_Picture" button, which you can use to add a slide with a picture background during a slide show. The picture file set for this button`n`n%General_Background_Picture_Path%`n`ndoes not exist.`n`nYou can change the path to the picture file in the .ini settings file, otherwise the button will be set to insert a blank white slide.`n`nClick "Yes" to quit the toolbar and edit the .ini file, or "No" to continue loading the program and use a blank white slide for this button.
			IfMsgBox Yes
				{
				Run, %General_IniFile%
				ExitApp
				}
			}
		}
	
	; Vertical toolbar
	If Toolbar_Direction = 0
		{
		;Distance from top
		If Button_Index > 1
			Y_Start := Y_Start + Toolbar_Button_Height + Toolbar_Button_Space
		
		; Check to see if button list contains unknown commands
		Current_Command = Command_%A_LoopField%
		If IsLabel(Current_Command) = 0
			{
			Msgbox, 0x40040,, The list of buttons for the toolbar defined under "Toolbar" -> "Button_Function" in the .ini file contains a button named "%A_LoopField%" as button number %Button_Index%.`n`nThis is not a recognized button and the toolbar will now exit.
			ExitApp
			}
			
		IfInString, A_LoopField, _Pen
			{
			NewStr1 := SubStr(Color_%A_LoopField%, 3, 2)
			NewStr2 := SubStr(Color_%A_LoopField%, 5, 2)
			NewStr3 := SubStr(Color_%A_LoopField%, 7, 2)
			Current_Color = %NewStr3%%NewStr2%%NewStr1%
			Gui, 1: Add, Progress, % " x" 0 " y" Y_Start " w" Toolbar_Button_Width " h" Toolbar_Button_Height " hwndToolbarButton_Progress" Button_Index " vButtonToolbar_Progress" Button_Index " Background" Color_Border " c" Current_Color " +Disabled", 100
			Gui, 1: Add, Text, % " xp" " y" Y_Start " w" Toolbar_Button_Width " h" Toolbar_Button_Height " hwndToolbarButton_Text" Button_Index " vButtonToolbar_Text" Button_Index " gCommand_" A_LoopField " Center +0x0200 BackgroundTrans"
			}
			
		Else If A_LoopField = Blank
			{
			Gui, 1: Add, Progress, % " x" 0 " y" Y_Start " w" Toolbar_Button_Width " h" Toolbar_Button_Height " hwndToolbarButton_Progress" Button_Index " vButtonToolbar_Progress" Button_Index " Background" Color_Background " c" Color_Background " +Disabled", 100
