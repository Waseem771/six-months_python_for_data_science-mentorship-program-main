				Gui, 1: Add, Picture, % " x" 0 " y" Y_Start " w" Toolbar_Button_Width " h" Toolbar_Button_Height " hwndToolbarButton_Picture" Button_Index " vButtonToolbar_Picture" Button_Index " gCommand_" A_LoopField " BackgroundTrans", %A_ScriptDir%\Toolbar_Icons\%A_LoopField%.png
			Else
				Gui, 1: Add, Picture, % " x" 0 " y" Y_Start " w" Toolbar_Button_Width " h" Toolbar_Button_Height " hwndToolbarButton_Picture" Button_Index " vButtonToolbar_Picture" Button_Index " gCommand_" A_LoopField " BackgroundTrans", % Icon_%A_LoopField%
			}
		}
	If Toolbar_Direction = 1
		{
		;Distance from top
