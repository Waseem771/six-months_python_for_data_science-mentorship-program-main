				{
				; Get status of laser
				ppt := ComObjActive("PowerPoint.Application")
				MyView := ppt.ActivePresentation.SlideShowWindow.View
				; If laser is on
				If MyView.LaserPointerEnabled = 1
					{
					; Reset pointer
					MyView.PointerType := 1
					; Set pointer to pen
					MyView.PointerType := 2
					Sleep, 100
					; Put laser back on
					MyView.LaserPointerEnabled := 1
					}
				; If laser is off
				Else
					{
					; Reset the pointer
					MyView.PointerType := 1
					MyView.PointerType := 2
					}
				}
			; If laser mode = 1, reset the pointer
			Else
				{
				MyView.PointerType := 1
				MyView.PointerType := 2
				}
			}
		; Set pointer as pen
		Else
			MyView.PointerType := 2
		
		ppt:=""
		}
	}
