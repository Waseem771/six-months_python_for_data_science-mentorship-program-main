Ajust_Seconds(Add_Seconds)
	{
	If Timer_1_State = 0
		{
		Timer_1_State = 1
		GuiControl, Timer_1: +c%Timer_1_Start_Color%, Timer_Button_Start
		GuiControl, Timer_1: , Timer_Start_Text, Start
		}
	SetTimer, Timer_1_Start, Off
	
	Total_Time_1 := Total_Time_1 + Add_Seconds
	If Total_Time_1 <= 0
		{
		Total_Time_1 = 0
		GoSub, Timer_1_Stop
		}
	Return Format("{:01}:{:02}", Total_Time_1//60, Mod(Total_Time_1, 60))
	}

Timer_1_Start:
If Timer_1_State = 0
	Return 
If Total_Time_1 = 0
	Return 
If Timer_1_Pause = 0
	{
	TimeSet_1:=A_TickCount
	SetTimer, Timer_1_Count, 50
	Timer_1_Pause = 1
	GuiControl, Timer_1: +c%Timer_1_Stop_Color%, Timer_Button_Stop
	GuiControl, Timer_1: , Timer_Stop_Text, Stop
	GuiControl, Timer_1: , Timer_Start_Text, Pause
	Hide_Buttons("Hide")
	}
Else
	{
	SetTimer, Timer_1_Count, OFF
	Timer_1_Pause = 0
	GuiControl, Timer_1: , Timer_Start_Text, Start
	Hide_Buttons("Show")
	}
Return 


Timer_1_Count:
If % (A_TickCount - TimeSet_1) >= 975
	{
	TimeSet_1:=A_TickCount
		
	Total_Time_1 := Total_Time_1 - 1
	
	If Total_Time_1 = 0
		{
		GuiControl, Timer_1: , Main_Text_1, % Format("{:01}:{:02}", Total_Time_1//60, Mod(Total_Time_1, 60))
		SetTimer, Timer_1_Count, OFF
		Timer_1_Pause = 0
		GoSub, Timer_1_Stop
		}	
	Else
		GuiControl, Timer_1: , Main_Text_1, % Format("{:01}:{:02}", Total_Time_1//60, Mod(Total_Time_1, 60))
	}
Return

